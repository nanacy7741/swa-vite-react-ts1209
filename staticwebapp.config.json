{
  "networking": {
    "allowedIpRanges": [
      "AzureFrontDoor.Backend"
    ]
  },
  "forwardingGateway": {
    "requiredHeaders": {
      "X-Azure-FDID": "eed73547-98f8-4654-9d58-802b17427721"
    },
    "allowedForwardedHosts": [
      "eaoiroawe.azurefd.net"
    ]
  },
  "routes": [
    {
      "route": "/admin/*",
      "allowedRoles": [
        "shirakawatest"
      ]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*.{js,map}",
      "/assets/*.{png,jpg,gif}",
      "/assets/*"
    ]
  }
}